<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dao.PaywarnrecordDAO">
	<insert id="add" parameterType="Paywarnrecord">
		insert into paywarnrecord values(null,#{status},#{notice},#{curtime})
	</insert>


	<select id="findById" resultType="Paywarnrecord" parameterType="int">
		select * from paywarnrecord where id=#{id}
	</select>
	<update id="update" parameterType="Paywarnrecord">
		update paywarnrecord set status=#{status} where id=#{id}
	</update>

</mapper>

